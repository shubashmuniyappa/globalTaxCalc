{
  "name": "geolocation-service",
  "version": "1.0.0",
  "description": "Geolocation and Tax Rules Service for GlobalTaxCalc",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "node src/scripts/seed.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "geolocation",
    "tax-rules",
    "geoip",
    "taxation",
    "location-detection"
  ],
  "author": "GlobalTaxCalc Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "redis": "^4.6.7",
    "maxmind": "^4.3.6",
    "geoip-lite": "^1.4.7",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "joi": "^17.9.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.10.0",
    "express-winston": "^4.2.0",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "csv-parser": "^3.0.0",
    "moment": "^2.29.4",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "@types/jest": "^29.5.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/scripts/**"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}