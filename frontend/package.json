{
  "name": "globaltaxcalc-frontend",
  "version": "1.0.0",
  "description": "Frontend application for GlobalTaxCalc - Next.js React application",
  "main": "next.config.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "export": "next export",
    "analyze": "ANALYZE=true next build",
    "i18n:extract": "formatjs extract 'src/**/*.{js,jsx,ts,tsx}' --out-file locales/messages.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile-folder --ast locales locales/compiled",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:sync": "node scripts/sync-translations.js",
    "i18n:auto-translate": "node scripts/auto-translate.js",
    "i18n:build": "npm run i18n:extract && npm run i18n:compile",
    "i18n:dev": "npm run i18n:extract && npm run i18n:compile && npm run dev"
  },
  "keywords": ["frontend", "nextjs", "react", "tailwindcss", "tax-calculator", "pwa"],
  "author": "GlobalTaxCalc Team",
  "license": "MIT",
  "dependencies": {
    "next": "^14.0.3",
