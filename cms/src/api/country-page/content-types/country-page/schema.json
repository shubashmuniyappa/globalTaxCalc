{
  "kind": "collectionType",
  "collectionName": "country_pages",
  "info": {
    "singularName": "country-page",
    "pluralName": "country-pages",
    "displayName": "Country Page",
    "description": "Country-specific landing pages with tax information and calculators"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "heroSection": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "overview": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "taxSystemOverview": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "keyTaxRates": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "importantDeadlines": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "popularDeductions": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "availableCalculators": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "flagImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "taxAuthorityInfo": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "commonQuestions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq-item.faq-item",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "relatedGuides": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tax-guide.tax-guide",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "relatedPosts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "lastUpdated": {
      "type": "datetime",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "priority": {
      "type": "integer",
      "required": false,
      "min": 1,
      "max": 10,
      "default": 5,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "featured": {
      "type": "boolean",
      "required": false,
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "currency": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "timeZone": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "language": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}